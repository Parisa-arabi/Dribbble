<!DOCTYPE html>  
<html lang="fa">  
<head>  
    <meta charset="UTF-8">  
    <meta name="viewport" content="width=device-width, initial-scale=1.0">  
    <link href="https://maxcdn.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css" rel="stylesheet">  
    <title>خریدهای من</title>  
</head>  
<body>  
    <div class="container">  
        <h1>خریدهای من</h1>  
        <table class="table">  
            <thead>  
                <tr>  
                    <th>عنوان طرح</th>  
                    <th>قیمت</th>  
                    <th>تاریخ خرید</th>  
                </tr>  
            </thead>  
            <tbody>  
                <% purchases.forEach(function(purchase) { %>  
                    <tr>  
                        <td><%= purchase.Title %></td>  
                        <td><%= purchase.Price %> تومان</td>  
                        <td><%= purchase.Date %></td>  
                    </tr>  
                <% }); %>  
            </tbody>  
        </table>  
    </div>  
</body>  
</html>